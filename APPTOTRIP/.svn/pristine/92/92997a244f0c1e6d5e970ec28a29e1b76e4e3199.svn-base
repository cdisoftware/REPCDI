<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="Circuito.aspx.cs" Inherits="CT.ADIM.WEB.Circuito" %>

<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>ATT - Circuito</title>
    <link href="MEDIA/CSS/bootstrap.min.css" rel="stylesheet" />
    <link href="MEDIA/CSS/portfolio-item.css" rel="stylesheet" />
    <link href="MEDIA/CSS/thumbnail-gallery.css" rel="stylesheet" />
    <link href="MEDIA/CSS/sb-admin-2.min.css" rel="stylesheet" />
    <link href="MEDIA/CSS/local.css" rel="stylesheet" />
    <script src="MEDIA/JS/jquery.min.js"></script>
    <script src="http://code.jquery.com/ui/1.10.3/jquery-ui.js"></script>
    <script src="MEDIA/JS/bootstrap.bundle.min.js"></script>
    <script src="MEDIA/JS/local.js"></script>
</head>
<body>
    <form id="form1" runat="server">
        <div style="width: 100%;" align="center">
            <div style="width: 70%;">
                <div class="row" style="padding:2.5px">
                    <div class="col-sm-2">
                        <label for="txtNSiri">Categoria:</label>
                    </div>
                    <div class="col-sm-2">
                        <asp:TextBox ID="txtNSiri" runat="server" onkeypress="return validar(event, 'val');" CssClass="form-control"></asp:TextBox>
                    </div>
                    <div class="col-sm-2">
                        <label for="txtNSiri">Titulo:</label>
                    </div>
                    <div class="col-sm-2">
                        <asp:TextBox ID="txtFechaInicio" runat="server" MaxLength="10" onkeypress="return validar(event, 'fecha');" CssClass="form-control" placeholder="Inicio"></asp:TextBox>
                    </div>
                    <div class="col-sm-2">
                        <label for="txtNSiri">Pais:</label>
                    </div>
                    <div class="col-sm-2">
                        <asp:TextBox ID="TextBox1" runat="server" MaxLength="10" onkeypress="return validar(event, 'fecha');" CssClass="form-control" placeholder="Fin" onblur="ValidaFecha()"></asp:TextBox>
                    </div>
                </div>
                <div class="row" style="padding:2.5px">
                    <div class="col-sm-2">
                        <label for="txtNSiri">Ciudad:</label>
                    </div>
                    <div class="col-sm-2">
                        <asp:TextBox ID="TextBox2" runat="server" onkeypress="return validar(event, 'val');" CssClass="form-control"></asp:TextBox>
                    </div>
                    <div class="col-sm-2">
                        <label for="txtNSiri">Tiempo Estimado:</label>
                    </div>
                    <div class="col-sm-2">
                        <asp:TextBox ID="TextBox3" runat="server" MaxLength="10" onkeypress="return validar(event, 'fecha');" CssClass="form-control" placeholder="Inicio"></asp:TextBox>
                    </div>
                    <div class="col-sm-2">
                        <label for="txtNSiri">Edad:</label>
                    </div>
                    <div class="col-sm-2">
                        <asp:TextBox ID="TextBox4" runat="server" MaxLength="10" onkeypress="return validar(event, 'fecha');" CssClass="form-control" placeholder="Fin" onblur="ValidaFecha()"></asp:TextBox>
                    </div>
                </div>
            </div>

        </div>
    </form>
</body>
</html>
