body {
    height: 100%;
    font-family: 'Century Gothic', sans-serif;
    background-image: linear-gradient(to top, #ffffff, rgba(204, 207, 221, 0.65));
}

.bg-body {
    background-image: url(../../MEDIA/IMG/bg-index.png);
    background-attachment: fixed;
    background-size: cover;
}

.modal-msg-img {
    background-color: white;
    border: solid 1px #333;
    left: 8%;
    opacity: 1;
    padding: 30px;
    position: fixed;
    border-radius: 10px 12px;
    top: 15%;
    visibility: hidden;
    width: 420px;
    z-index: 1000;
}

.modal-msg {
    background-color: white;
    border: solid 1px #333;
    left: 38%;
    opacity: 1;
    padding: 30px;
    position: fixed;
    border-radius: 10px 12px;
    top: 33%;
    visibility: hidden;
    width: 420px;
    z-index: 1000;
}

.div-center {
    align-items: center;
    display: flex;
    justify-content: center;
}

.form-control-obligatorio {
    color: #ff0000;
    background-color: #fff;
    border-color: #ff7676;
    box-shadow: 0 0 5px rgba(254, 4, 4, 0.36);
    outline: 0;
}



.lblMensajePU {
    font-size: 14px;
    color: #333333;
}

a.popup-cerrar {
    position: absolute;
    border: solid 1px white;
    top: 3px;
    right: 3px;
    border-radius: 8px 8px;
    background-color: #333;
    padding: 3px 6px;
    font-size: 20px;
    text-decoration: none;
    line-height: 1;
    color: #fff;
}

.col-lg-3-l {
    position: relative;
    width: 100%;
    padding-right: 5px;
    padding-left: 5px;
}

.file-upload {
    display: inline-block;
    overflow: hidden;
    text-align: center;
    vertical-align: top;
    font-family: Arial;
    /*border: 1px solid #124d77;*/
    background: url(../../MEDIA/IMG/UploadFileBig.png) center no-repeat;
    color: #fff;
    border-radius: 6px;
    -moz-border-radius: 6px;
    cursor: pointer;
    text-shadow: #000 1px 1px 2px;
    -webkit-border-radius: 6px;
}

    .file-upload:hover {
        /*background: -webkit-gradient(linear, left top, left bottom, color-stop(0.05, #0061a7), color-stop(1, #007dc1));
                background: -moz-linear-gradient(top, #0061a7 5%, #007dc1 100%);
                background: -webkit-linear-gradient(top, #0061a7 5%, #007dc1 100%);
                background: -o-linear-gradient(top, #0061a7 5%, #007dc1 100%);
                background: -ms-linear-gradient(top, #0061a7 5%, #007dc1 100%);
                background: linear-gradient(to bottom, #0061a7 5%, #007dc1 100%);
                filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#0061a7', endColorstr='#007dc1',GradientType=0);*/
        /*background-color: #0061a7;*/
    }

.file-upload {
    height: 200px;
}

    .file-upload, .file-upload span {
        width: 100%;
    }

        .file-upload input {
            top: 0;
            left: 0;
            margin: 0;
            font-size: 11px;
            font-weight: bold;
            /* Loses tab index in webkit if width is set to 0 */
            opacity: 0;
            filter: alpha(opacity=0);
        }

        .file-upload strong {
            font: normal 12px Tahoma,sans-serif;
            text-align: center;
            vertical-align: middle;
        }

        .file-upload span {
            top: 0;
            left: 0;
            display: inline-block;
            /* Adjust button text vertical alignment */
            padding-top: 5px;
        }

