/*---------------------
	Header section
-----------------------*/
.header-section {
    /*background-image: url(../IMG/bg-header.png);*/
    background-position-x: center;
    background-position-y: center;
    background-size: contain;
    /*background-repeat:no-repeat;*/
    padding: 5px 20px;
    /*background: #f2f7f8;*/
}

.site-logo h2 {
    font-size: 36px;
}

    .site-logo h2 a {
        color: #40424a;
    }

.site-logo p {
    font-size: 14px;
    line-height: normal;
}

.header-buttons a {
    margin-top: 5px;
    font: bold 15px arial, verdana;
}

/*---------------------
	Hero section
-----------------------*/
.hero-section {
    /*background: #f2f7f8;*/
    /*background-color:#ffffff;*/
    width: 100%;
}

.hero-text {
    margin-bottom: 5px;
}

    .hero-text h2 {
        font-size: 50px;
        line-height: normal;
        margin-bottom: 5px;
    }

    .hero-text p {
        font-size: 33px;
        line-height: 1.3;
    }

.hero-image {
    padding-top: 70px;
    padding-left: 70px;
    border-radius: 50% 50%;
}

.hero-info h2 {
    font-size: 20px;
    color: #999999;
    margin-bottom: 30px;
}

.hero-info ul {
    list-style: none;
}

/*---------------------
	Social section
-----------------------*/

.img-personal {
    background-image: url(../IMG/NoImage.png);
    background-repeat: no-repeat;
    background-position-x: center;
    background-position-y: center;
    width: 200px;
    height: 200px
}

.social-section {
    /*background: #f9f9f9;*/
    background-color: #fff;
    padding: 80px 0;
}

    .social-section .social-links {
        display: inline-block;
        background: #f9f9f9;
        position: relative;
        z-index: 5;
    }

        .social-section .social-links a {
            color: #484848;
            font-size: 36px;
            margin-right: 40px;
        }

    .social-section .social-link-warp {
        position: relative;
    }

        .social-section .social-link-warp h2 {
            display: inline-block;
            float: right;
            font-weight: 400;
            padding-left: 30px;
            background: #f9f9f9;
            position: relative;
            z-index: 5;
        }

        .social-section .social-link-warp:after {
            position: absolute;
            content: '';
            width: 50%;
            height: 2px;
            left: 23%;
            top: 50%;
            margin-top: 1px;
            background: #cbcbcb;
        }
